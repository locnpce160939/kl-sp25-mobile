{
  "expo": {
    "jsEngine": "hermes",
    "name": "kl-sp25-app",
    "slug": "kl-sp25-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "The app needs access to your photos to upload ID card images.",
          "cameraPermission": "The app needs access to your camera to capture ID card images."
        }
      ]
    ],
    "ios": {
      "jsEngine": "jsc",
      "supportsTablet": true,
      "config": {
        "googleMapsApiKey": "t0vRyftUba3uIEnx5JlMJta2ff3B03BEUVg0xHWw"
      },
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "Ứng dụng cần quyền truy cập vị trí của bạn để hiển thị vị trí hiện tại.",
        "NSCameraUsageDescription": "Ứng dụng cần quyền truy cập camera để chụp ảnh.",
        "NSPhotoLibraryUsageDescription": "Ứng dụng cần quyền truy cập vào thư viện ảnh để chọn ảnh.",
        "NSPhotoLibraryAddUsageDescription": "Ứng dụng cần quyền để lưu ảnh vào thư viện."
      }
    },
    "android": {
      "enableHermes": true,
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "config": {
        "googleMaps": {
          "apiKey": "t0vRyftUba3uIEnx5JlMJta2ff3B03BEUVg0xHWw"
        }
      },
      "permissions": [
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.RECORD_AUDIO"
      ],
      "package": "com.locnpce160939.klsp25app"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "a2e953dc-8353-41a3-85e4-e25427c86dfb"
      }
    }
  }
}
